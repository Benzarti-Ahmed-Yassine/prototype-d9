{
  "name": "mediflow-platform",
  "version": "1.0.0",
  "description": "Plateforme numérique de gestion des médicaments avec blockchain Hedera",
  "keywords": [
    "healthcare",
    "blockchain",
    "hedera",
    "prescriptions",
    "medical",
    "digital-health"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/mediflow-platform.git"
  },
  "license": "MIT",
  "author": "MediFlow Team",
  "main": "api/index.js",
  "scripts": {
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "start": "node api/index.js",
    "vercel-build": "cd frontend && npm install && npm run build"
  },
  "dependencies": {
    "@hashgraph/sdk": "latest",
    "@testing-library/dom": "latest",
