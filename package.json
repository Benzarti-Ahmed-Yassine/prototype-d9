    "bcryptjs": "latest",
    "bn.js": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "latest",
    "dotenv": "latest",
    "express": "latest",
    "geist": "latest",
    "jiti": "latest",
    "jsonwebtoken": "latest",
    "less": "latest",
    "lightningcss": "latest",
    "lucide-react": "^0.454.0",
    "next": "15.2.4",
    "next-themes": "latest",
    "path": "latest",
    "react": "^19",
    "react-dom": "^19",
    "react-router-dom": "latest",
    "sass": "latest",
    "sass-embedded": "latest",
    "sequelize": "latest",
